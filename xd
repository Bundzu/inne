package javaapplication26;
import java.util.*;

public class JavaApplication26 {

    public static void main(String[] args) {
       Scanner in = new Scanner(System.in);
        System.out.println("podaj liczbe");
       int j = in.nextInt();
       int[] table;
       table = new int[j];
      for(int i=0;i<j;i++){
          System.out.println("Podaj liczbe");
          table[i]=in.nextInt();
      }

    Arrays.sort(table);
for(int i=0;i<j;i++){
          System.out.println(table[i]);
          
      }
    }
    
}

